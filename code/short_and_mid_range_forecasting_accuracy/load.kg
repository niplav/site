.l("csv")
.l("nstat")

spd::24*60*60
spw::7*spd
spm::30*spd
spy::365*spd

brier::{mu((x-y)^2)}

.comment("******")

.fc(.ic("../../data/pb.csv"));pbraw::csv.load()

pbdata::+{(1:$*x),1.0:$'x}'pbraw

:"For days"
dpbdiffs::{_x%spd}pbdata@3
:"For weeks"
wpbdiffs::{_x%spw}'pbdata@3
:"For months (roughly)"
mpbdiffs::{_x%spm}'pbdata@3
"For years"
ypbdiffs::{_x%spy}'pbdata@3

pbress::pbdata@1
pbfcs::pbdata@2

dpbdg::=dpbdiffs
dpbdiffbrier::{(dpbdiffs@*x),mse.set(pbfcs@x;pbress@x)}'dpbdg
dpbdiffbrier::dpbdiffbrier@<*'dpbdiffbrier

wpbdg::=wpbdiffs
wpbdiffbrier::{(wpbdiffs@*x),mse.set(pbfcs@x;pbress@x)}'wpbdg
wpbdiffbrier::wpbdiffbrier@<*'wpbdiffbrier

mpbdg::=mpbdiffs
mpbdiffbrier::{(mpbdiffs@*x),mse.set(pbfcs@x;pbress@x)}'mpbdg
mpbdiffbrier::mpbdiffbrier@<*'mpbdiffbrier

ypbdg::=ypbdiffs
ypbdiffbrier::{(ypbdiffs@*x),mse.set(pbfcs@x;pbress@x)}'ypbdg
ypbdiffbrier::ypbdiffbrier@<*'ypbdiffbrier

